[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
EnvironmentFile=/opt/Django_CICD/.env_config
User=root
Group=www-data
WorkingDirectory=/opt/Django_CICD/app
ExecStart=/opt/Django_CICD/${ENV_NAME}/bin/gunicorn --workers 3 --log-level debug --error-logfile /opt/Django_CICD/logs/error.log --bind unix:/run/gunicorn.sock app.wsgi:application


[Install]
WantedBy=multi-user.target


